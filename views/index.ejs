<%- include('header') -%>
<% var historyList = JSON.parse(history) %>
<% var summaryList = JSON.parse(summary) %>

<div class="site-blocks-cover overlay" style="background-image: url(images/logo.png);" data-aos="fade"
  data-aos-delay="200" data-stellar-background-ratio="0.5">
  <div class="container">
    <div class="row align-items-center justify-content-center text-center">
      <div class="col-md-12" data-aos="fade-up" data-aos-delay="200">

        <div class="row justify-content-center mb-4">
          <div class="col-md-8 text-center">
            <strong>
              <h1 style="color: rgb(255, 60, 60);"><%= welcome %> </h1>
            </strong>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="container" style="height: 3rem;"></div>

<section id="history-section" class="site-section" data-aos="fade">
  <div class="container">
    <div class="row jsutify-content-center mb-5 text-center">
      <div class="col-md-6 mlg-4 col">
        <table class="table">
          <% for(var i =0; i < historyList.length; i++) {%>
          <tbody>
            <td><%= historyList[i].year %> </td>
            <td><%= historyList[i].content %> </td>
          </tbody>
          <% } %>
        </table>
      </div>
      <div class="col-md-6 mlg-4 col">
        <img src="" alt="chart">
      </div>
    </div>
  </div>
</section>
<section id="summary-section" class="site-section" data-aos="fade">
  <div class="container">
    <div class="text-center" style="margin-bottom: 4rem;">
      <h2>주요서비스</h2>
    </div>
  </div>
  <div class="container">
    <ul>
      <% for(var i=0; i<summaryList.length;i++){ %>
      <li>
        <h4><%= summaryList[i].title %> </h4>
        <p><%= summaryList[i].description %> </p>
      </li>
      <% } %>
    </ul>
  </div>
  <section>

  </section>

  <%- include('footer') -%>